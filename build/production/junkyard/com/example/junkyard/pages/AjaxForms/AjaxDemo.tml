<html t:type="layout" title="Create New Address"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd">
<div class="container">
    <t:form t:id="editAddressForm">

        <div class="four columns">

            <t:label for="firstName"/>
            <t:textfield t:id="firstName" class="input-text" t:validate="required"/>

            <label>street 1</label>
            <t:textfield t:id="street1" class="input-text" t:validate="required"/>


            <label>street 2</label>
            <t:textfield t:id="street2" class="input-text" t:validate="required"/>

            <label>City</label>
            <t:textfield t:id="city" class="input-text" t:validate="required"/>

            <label>State</label>
            <t:textfield t:id="state" class="input-text" t:validate="required"/>

            <label>Zip</label>
            <t:textfield t:id="zip" class="input-text" t:validate="required"/>

            <label>Email</label>
            <t:textfield t:id="email" class="input-text" t:validate="required"/>

            <label>Phone</label>
            <t:textfield t:id="phone" class="input-text" value="${phone}"/>


        </div>


        <div class="three columns">
            <fieldset>
                <label for="radio1"><input type="radio" id="radio1" name="radio1"/><span class="custom radio"/> Radio
                    Button 1</label>
                <label for="radio2"><input type="radio" id="radio2" name="radio1"/><span class="custom radio checked"/>
                    Radio Button 2</label>
                <label for="radio3"><input type="radio" id="radio3" name="radio1"/><span class="custom radio"/> Radio
                    Button 3</label>
                <BR></BR>
            </fieldset>

            <fieldset>
                <label for="checkbox1"><input type="checkbox" id="checkbox1"/><span class="custom checkbox"/> Label for
                    Checkbox</label>

                <label for="checkbox2"><input type="checkbox" checked="" id="checkbox2"/><span
                        class="custom checkbox checked"/> Label for Checkbox</label>
                <label for="checkbox3"><input type="checkbox" id="checkbox3" checked=""/><span
                        class="custom checkbox "/> Label for Checkbox</label>
            </fieldset>

            <fieldset>
                <t:label for="carMake"/>
                : <select t:type="select" t:id="carMake" t:model="carMakes" t:zone="carModelZone"
                        t:blankOption="ALWAYS" t:blankLabel="Choose..." t:validate="required"/>

                <t:zone t:id="carModelZone" id="carModelZone" style="display: inline;">
                    <t:label for="carModel"/>  :
                    <select t:type="select" t:id="carModel" t:model="carModels"
                            t:blankOption="ALWAYS" t:blankLabel="Choose..." t:validate="required"/>
                </t:zone>

            </fieldset>
        </div>
        <BR clear="all"></BR>

        <div class="row">

            <div class="seven columns">
                <t:submit t:id="Update" class="nice medium radius blue button" value="Update"/>
                <P></P>

                <t:pagelink class="nice medium radius black button"
                            page="formsdemo/foundationformsdemo">Exit
                </t:pagelink>

            </div>
        </div>
    </t:form>

</div>


</html>